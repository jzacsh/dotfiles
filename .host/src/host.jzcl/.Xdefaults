
! --[ Font Rendering ]-------------------------------------
Xft.dpi: 96
Xft.antialias: true
! Xft.rgba: rgb
! Xft.hinting: true
! Xft.hintstyle: hintslight
! Xft.lcdfilter: lcddefault

! --[ Rodent ]----------------------------------------------
Xcursor.theme: Vanilla-DMZ
Xcursor.size:  18

! --[ URxvt - General ]-------------------------------------
URxvt.depth: 32
URxvt.scrollBar:false
URxvt.saveLines:5000
URxvt.cursorColor:#83C048
URxvt.highlightColor:#4e4e4e
URxvt.font: xft:DroidSansMono:pixelsize=9
URxvt.boldFont: xft:DroidSansMono:pixelsize=9:bold
URxvt.loginshell: true
URxvt.internalborder: 0
URxvt.underlinecolor: #999999
URxvt.cutchars: "()*,<>[]{}|'"
URxvt.print-pipe: cat >/tmp/xxx
URxvt.visualBell: false

! --[ URxvt - URL Handling ]--------------------------------
URxvt.keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt.perl-ext: selection,mark-yank-urls
URxvt.urlLauncher: chromium
URxvt.underlineURLs: true

! --[ URxvt - colors ]--------------------------------------
URxvt.background: rgba:1111/1111/1111/eeee
URxvt.foreground:#eeeeee

